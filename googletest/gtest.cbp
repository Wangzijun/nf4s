<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gtest" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug Win32">
				<Option output=".objs/gtestd" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-W" />
					<Add option="-O0" />
					<Add option="-pthread" />
					<Add option="-DWIN32" />
					<Add option="-D_DEBUG" />
					<Add option="-D_LIB" />
					<Add directory="." />
					<Add directory="./include" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-D_WIN32_WINNT=0x0500" />
		</Compiler>
		<Unit filename="src/gtest-all.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
